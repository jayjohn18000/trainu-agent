            "delay": 0
          },
          {
            "step": 3,
            "action": "start_nurture_sequence",
            "sequenceId": "funnel_nurture",
            "delay": 0
          },
          {
            "step": 4,
            "action": "notify_sales",
            "delay": 0
          },
          {
            "step": 5,
            "action": "add_to_pipeline",
            "pipelineId": "sales_pipeline",
            "stage": "Lead",
            "delay": 0
          }
        ]
      },
      {
        "name": "Website Lead Capture",
