  "workflows": {
    "definitions": [
      {
        "name": "Missed Call Follow-up",
        "trigger": "missed_call",
        "conditions": [],
        "actions": [
          {
            "type": "send_sms",
            "template": "missed_call_followup",
            "delay": 300
          },
          {
            "type": "add_tag",
            "value": "follow_up_needed"
          }
        ]
      },
      {
        "name": "Appointment Reminder - 24h Before",
        "trigger": "appointment_scheduled",
        "conditions": [
          {
            "field": "appointment.startTime",
            "operator": "hours_until",
            "value": 24
          }
        ],
        "actions": [
          {
            "type": "send_sms",
            "template": "appointment_reminder_24h",
            "delay": 0
          },
          {
            "type": "send_email",
            "template": "appointment_reminder_email",
            "delay": 0
          }
        ]
      },
      {
        "name": "Birthday Greeting",
        "trigger": "contact_birthday",
        "conditions": [],
        "actions": [
          {
            "type": "send_sms",
            "template": "birthday_greeting",
            "delay": 0
          },
          {
            "type": "send_email",
            "template": "birthday_email",
            "delay": 0
          }
        ]
      },
      {
        "name": "At-Risk Client Recovery",
        "trigger": "tag_added",
        "conditions": [
          {
            "field": "tag",
            "operator": "equals",
            "value": "at_risk"
          }
        ],
        "actions": [
          {
            "type": "send_email",
            "template": "at_risk_recovery",
            "delay": 0
          },
          {
            "type": "send_sms",
            "template": "at_risk_sms",
            "delay": 3600
          },
          {
            "type": "add_tag",
            "value": "recovery_campaign"
          }
        ]
      },
      {
        "name": "Survey Request After Session",
        "trigger": "appointment_completed",
        "conditions": [],
        "actions": [
          {
            "type": "send_email",
            "template": "session_survey",
            "delay": 3600
          },
          {
            "type": "add_tag",
            "value": "survey_sent"
          }
        ]
      },
      {
        "name": "Pipeline Stage Change Notification",
        "trigger": "pipeline_stage_changed",
        "conditions": [],
        "actions": [
          {
            "type": "notify_user",
            "message": "Pipeline stage changed for {{contact.firstName}}: {{pipeline.newStage}}"
          },
          {
            "type": "update_forecast",
            "stage": "{{pipeline.newStage}}"
          },
          {
            "type": "send_email",
            "template": "pipeline_update",
            "delay": 0
          }
        ]
      },
      {
        "name": "Review Request Automation",
        "trigger": "appointment_completed",
        "conditions": [],
        "actions": [
          {
            "type": "request_review",
            "delay": 86400
          },
          {
            "type": "add_tag",
            "value": "review_requested"
          },
          {
            "type": "create_task",
            "title": "Follow up on review request for {{contact.firstName}}",
            "dueDate": "+3 days",
            "delay": 259200
          }
        ]
      },
      {
        "name": "Review Received - Thank You",
        "trigger": "review_received",
        "conditions": [],
        "actions": [
          {
            "type": "send_email",
            "template": "review_thank_you",
            "delay": 0
          },
          {
            "type": "add_tag",
            "value": "review_received"
          }
        ]
      },
      {
        "name": "Social Media Post Scheduler",
        "trigger": "scheduled_post",
        "conditions": [],
        "actions": [
          {
            "type": "publish_to_facebook",
            "delay": 0
          },
          {
            "type": "publish_to_instagram",
            "delay": 0
          },
          {
            "type": "track_engagement",
            "delay": 0
          }
        ]
      }
    ],
    "references": []
  },
  "automations": {
    "definitions": [
