          {
            "step": 1,
            "action": "send_email",
            "template": "campaign_announcement",
            "delay": 0
          },
          {
            "step": 2,
            "action": "send_sms",
            "template": "campaign_sms",
            "delay": 3600
          },
          {
            "step": 3,
            "action": "send_messenger",
            "template": "campaign_messenger",
            "delay": 7200
          },
          {
            "step": 4,
            "action": "publish_social_post",
            "platforms": ["facebook", "instagram"],
            "delay": 0
          }
        ]
