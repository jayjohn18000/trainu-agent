        "steps": [
          {
            "step": 1,
            "action": "create_contact",
            "delay": 0
          },
          {
            "step": 2,
            "action": "add_to_pipeline",
            "pipelineId": "sales_pipeline",
            "stage": "Lead",
            "delay": 0
          },
          {
            "step": 3,
            "action": "send_welcome_sequence",
            "delay": 0
          },
          {
            "step": 4,
            "action": "assign_tag",
            "value": "website_lead",
            "delay": 0
          }
        ]
      },
